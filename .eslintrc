{
	"parser": "babel-eslint",
	"env": {
		"browser": true
	},
  "plugins": [
    "es5"
	],
	"rules": {
		"semi": 2,
		"quotes": 2,
		"quote-props": 2,
		"template-tag-spacing": 2,
		"no-console": 1,
		"no-multi-spaces": 2,
		"space-in-parens": 2,
		"no-multi-str": 2,
		"block-spacing": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-trailing-spaces": 2,
		"no-whitespace-before-property": 2,
		"space-before-blocks": 2,
		"space-before-function-paren": 2,
		"space-infix-ops": 2,
		"space-unary-ops": 
		2,
		"spaced-comment": 2
	}
}